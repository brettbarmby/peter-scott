<div class="page-body">
  <div class="body-left">
    <%= render 'shared/navbar' %>
  </div>
  <div class="gallery">
    <div class="slideshow-container">

  <!-- Full-width images with number and caption text -->
      <div class="mySlides fade">
        <img src="/assets/2004-2009/barkerville_mountain_11x14_lg.jpg" alt="Barkerville Mountain" style="width: 600px;">
        <div class="text">"Barkerville Mountain"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/Cariboo Mountains_12x16_lg.jpg" alt="Cariboo Mountains" style="width: 600px;">
        <div class="text">"Cariboo Mountains"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/cascade_mountains_lg.jpg" alt="Cascade Mountains" style="width: 600px;">
        <div class="text">"Cascade Mountains"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/columbia_glacier_lg.jpg" alt="Columbia Glacier" style="width: 600px;">
        <div class="text">"Columbia Glacier"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/fools_gold_lg.jpg" alt="Fool's Gold" style="width: 600px;">
        <div class="text">"Fool's Gold"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/on_the_river_lg.jpg" alt="On the River" style="width: 600px;">
        <div class="text">"On the River"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/springtime_rockies_11x14_lg.jpg" alt="Springtime Rockies" style="width: 600px;">
        <div class="text">"Springtime Rockies"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/whistler_mtn_lg.jpg" alt="Whistler Mountain" style="width: 600px;">
        <div class="text">"Whistler Mountain"</div>
      </div>

      <div class="mySlides fade">
        <img src="/assets/2004-2009/wolverine_mtn_lg.jpg" alt="Wolverine Mountain" style="height: 500px;">
        <div class="text">"Wolverine Mountain"</div>
      </div>


      <!-- Next and previous buttons -->
      <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
      <a class="next" onclick="plusSlides(1)">&#10095;</a>
    </div>
    <br>

    <!-- The dots/circles -->
    <div class="dot-row" style="text-align:center">
      <span class="dot" onclick="currentSlide(1)"></span>
      <span class="dot" onclick="currentSlide(2)"></span>
      <span class="dot" onclick="currentSlide(3)"></span>
      <span class="dot" onclick="currentSlide(4)"></span>
      <span class="dot" onclick="currentSlide(5)"></span>
      <span class="dot" onclick="currentSlide(6)"></span>
      <span class="dot" onclick="currentSlide(7)"></span>
      <span class="dot" onclick="currentSlide(8)"></span>
      <span class="dot" onclick="currentSlide(9)"></span>
    </div>
  </div>
</div>
<div class="footer">
  <%= render 'shared/footer' %>
</div>

